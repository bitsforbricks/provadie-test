<html lang="nl">

<head>
  <meta http-equiv=Content-Type content="text/html; charset=UTF-8">
  <style type="text/css">
    html,
    body {
      padding: 0;
      margin: 0;
    }

    .map {
      height: 100vh;
      text-align: center;
      width: 100%;
    }
  </style>
</head>

<body>
  <div class="map" id="map"></div>
  <script src="https://maps.googleapis.com/maps/api/js?key=<%= mapsApiKey %>&libraries=marker&v=quarterly"></script>
  <script>
    var maxZoom = 18;
    var containerElement = document.getElementById("map");
    var map = new google.maps.Map(
      containerElement,
      {
        maxZoom,
        mapTypeId: '<%= mapTypeId %>',
        disableDefaultUI: true,
        // Can be found in https://console.cloud.google.com/google/maps-apis/studio/maps DefaultProvadieGoogleMapsStyle
        mapId: '<%= mapId %>',
      }
    );
  </script>
</body>

</html>